{
	"name": "Package Update Notifier",
	"type": "interval",
	"description": "Tracks pip/npm/cargo package updates",
	"endpoint": "https://your-domain.com/health",
	"config": {
		"interval": "daily",
		"package_managers": ["pip", "npm", "cargo"]
	},
	"auth": {
		"type": "api_key",
		"key": "X-API-Key"
	}
}
